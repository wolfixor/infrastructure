# Nexus Repository Configuration
# Original file backed up automatically
{% if 'security' in item.path %}
deb {{ nexus_repo_url }} bookworm-security main
{% elif 'backports' in item.path %}
deb {{ nexus_repo_url }} bookworm-backports main
{% elif 'updates' in item.path %}
deb {{ nexus_repo_url }} bookworm-updates main
{% elif 'docker' in item.path %}
deb {{ nexus_repo_url }} bookworm stable
{% else %}
deb {{ nexus_repo_url }} bookworm main
{% endif %}